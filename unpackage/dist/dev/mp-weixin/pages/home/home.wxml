<view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><view class="swiper-item"><image src="{{item.image_src}}"></image></view></swiper-item></block></swiper><view class="nav-list"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="nav-item"><image class="nav-img" src="{{item.image_src}}"></image></view></block></view><block wx:for="{{floorList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="floor-list"><view class="floor-item"><image class="floor-title" src="{{item.floor_title.image_src}}"></image></view><view class="floor-img-box"><view class="left-img-box"><image style="{{'width:'+(item.product_list[0].image_width+'rpx')+';'}}" src="{{item.product_list[0].image_src}}" mode="widthFix"></image></view><view class="right-img-box"><block wx:for="{{item.product_list}}" wx:for-item="item2" wx:for-index="index2" wx:key="index2"><block wx:if="{{index2!=0}}"><view class="right-img-item"><image style="{{'width:'+(item2.image_width+'rpx')+';'}}" src="{{item2.image_src}}" mode="widthFix"></image></view></block></block></view></view></view></block></view>